     {\global\advance \@alcnt \@ne
       \expandafter\xdef\csname @al#3\endcsname{\the\@alcnt}}%
     {\relax}%
   \fi
    \ifnum\@alcnt=\@ne
     \gdef\@alone{#3}%
    \fi
   \@checkword{#1}%
   \setbox\@tempboxc\hbox{\@Getword}%
   \ifdim \wd\@tempboxc=1zw
    \setbox\@tempboxc\hbox{\spaceskip=100mm #1}%
    \ifdim \wd\@tempboxc < 100mm
     \typeout{Style jsai Warning: Author's name "#1" doesn't have a space.}%
    \else
     \setbox\@tempboxc\hbox{#1}%
     \def\temp@type{COM}%
     \ifx\np@type\temp@type
      \ifdim \wd\@tempboxc > 5zw
       \expandafter\def\csname @authorname\number\@authorcnt\endcsname{%
        \leavevmode
         \vtop{\hbox{{\spaceskip=1zw #1}%
              $^{*\mbox{\sixrm \csname @al#3\endcsname}}$}%
               \nointerlineskip\vskip1mm
               \hbox{\EFsubauthorfont #2}}%
       }%
      \else
       \expandafter\def\csname @authorname\number\@authorcnt\endcsname{%
        \leavevmode
         \vtop{\hbox{\hbox to 6zw{\kanjiskip.5zw
          \spaceskip1zw plus 1fill minus 1fill #1}%
                $^{*\mbox{\sixrm \csname @al#3\endcsname}}$}%
               \nointerlineskip\vskip1mm
               \hbox{\EFsubauthorfont #2}}%
       }%
      \fi
     \else
      \def\temp@type{FOR}%
      \ifdim \wd\@tempboxc > 4.5zw
       \expandafter\def\csname @authorname\number\@authorcnt\endcsname{%
        \leavevmode
         \vtop{\hbox{{\spaceskip=1zw \blindsw@jn{#1}}%
               $^{*\mbox{\sixrm \csname @al#3\endcsname}}$}%
               \ifx\np@type\temp@type\else
                \nointerlineskip\vskip1mm
                \hbox{\EFsubauthorfont \blindsw@en{#2}}%
               \fi
         }%
       }%
      \else
       \expandafter\def\csname @authorname\number\@authorcnt\endcsname{%
        \leavevmode
         \vtop{\hbox{\hbox to 5zw{\spaceskip\z@ plus 1fill minus 1fill 
           \blindsw@jn{#1}}%
