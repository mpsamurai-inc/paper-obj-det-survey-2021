    \@ifundefined{@al#3}%
     {\global\advance \@alcnt \@ne
       \expandafter\xdef\csname @al#3\endcsname{\the\@alcnt}}%
     {\relax}%
    \@ifundefined{@nl#4}%
     {\global\advance \@ntcnt \@ne%
      \expandafter\xdef\csname @nl#4\endcsname{\the\@ntcnt}}%
     {\relax}%
   \fi
   \ifnum\@alcnt=\@ne
    \gdef\@alone{#3}%
   \fi
   \if@titlenote
   \else
    \ifnum\@ntcnt=\@ne
     \gdef\@ntone{#4}%
