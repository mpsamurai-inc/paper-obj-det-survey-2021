\def\@secjidori#1#2{%
 \if@english #2\else
  \def\@tempfn{}\def\@tempfncnt{}%
  \@tempskipb #1\relax
  \bgroup
   \def\footnote{\@ifnextchar[{\@ignorefn}{\@ignorefn[]}}% ]
   \long\def\@ignorefn[##1]##2{\gdef\@tempfncnt{##1}\gdef\@tempfn{##2}}%
   \def\footnotemark{\@ifnextchar[{\@ignorefnmark}{\@ignorefnmark[]}}% ]
   \def\@ignorefnmark[##1]{}%
   \global\setbox\@tempboxb\hbox{#2}%
  \egroup
  \ifdim \@tempskipb>\z@
   \ifdim \wd\@tempboxb>6.5zw 
    \inhibitglue #2%
   \else
    \def\@makefnmark{\hbox to \z@{$^{*\@thefnmark}$\hss}}%
    \kanjiskip\z@ plus 1fill minus 1fill \xkanjiskip\kanjiskip
     \jmathkanjiskip\kanjiskip
    \ifdim \wd\@tempboxb>2.5zw
     \hbox to 7zw{\let\@footnotetext\@gobble\inhibitglue #2\hfil}%
    \else
     \hbox to 5zw{\let\@footnotetext\@gobble\inhibitglue #2\hfil}%
    \fi
     \xkanjiskip.25zw plus .15zw minus .1zw
     \kanjiskip0pt plus .4pt minus .5pt
     \ifx\@tempfn\empty\else
      \ifx\@tempfncnt\empty\footnotetext{\@tempfn}\else
       \footnotetext[\@tempfncnt]{\@tempfn}%
      \fi\fi
   \fi
  \else
   \ifdim \wd\@tempboxb>2.5zw
    \inhibitglue #2%
   \else
    \def\@makefnmark{\hbox to \z@{$^{*\@thefnmark}$\hss}}%
    \kanjiskip\z@ plus 1fill minus 1fill \xkanjiskip\kanjiskip
     \jmathkanjiskip\kanjiskip
    \hbox to 3zw{\let\@footnotetext\@gobble\inhibitglue #2\hfil}%
     \xkanjiskip.25zw plus .15zw minus .1zw
     \kanjiskip0pt plus .4pt minus .5pt
     \ifx\@tempfn\empty\else
      \ifx\@tempfncnt\empty\footnotetext{\@tempfn}\else
       \footnotetext[\@tempfncnt]{\@tempfn}%
      \fi\fi
   \fi
  \fi
  \gdef\@tempfn{}\gdef\@tempfncnt{}%
 \fi
}

